<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta http-equiv="x-ua-compatible" content="ie=edge" />

    <title>@ViewData["Title"] - Student Attendance</title>
    <meta content="" name="description">
    <meta content="" name="keywords">
    <meta name="csrf-token" content="{{ csrf_token() }}">
    <!-- Favicons -->
    <link href="~/lib/templates/NiceAdmin/assets/img/favicon.png" rel="icon">
    <link href="~/lib/templates/NiceAdmin/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <partial name="NiceAdmin/_Styles" />
    @await RenderSectionAsync("Styles", false)
</head>

<body>
    <partial name="NiceAdmin/_Header" />
    <partial name="NiceAdmin/_Sidebar" />

    <main id="main" class="main">
        @RenderBody()
    </main>

    <partial name="NiceAdmin/_Footer" />
    <partial name="NiceAdmin/_Scripts" />
    @await RenderSectionAsync("Scripts", required: false)

</body>

</html>
